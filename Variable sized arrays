#include <iostream>
using namespace std;

int main() 
{
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n,q,k,s=0,m,l;
    cin>>n>>q;
    int *arr[n];//variable sized array
    while(n--)//2//1
    {
        cin>>k;//3.....5
        arr[s]=new int[k];//3'...a[0].....5'
        for(int i=0;i<k;i++)
        {
        cin>>arr[s][i];
        }
    
        s++;
       
    }
        
    while(q--)
    {
     
        cin>>m>>n;
        cout<<arr[m][n]<<endl;
    }
    
    return 0;
}
