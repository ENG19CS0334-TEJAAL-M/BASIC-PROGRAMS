#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
     int a[10000],n,i;
     
     scanf("%d",&n);
     for(i=1;i<=n;i++)
     {
         scanf("%d ",&a[i]);
     }
     for(i=n;i>0;i--)
     {
         printf("%d ",a[i]);
     }
    return 0;
}
