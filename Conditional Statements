#include<iostream>
using namespace std;



int main()
{
    int a;
    cin>>a;
    for(int i=1;i<=9;i++)
    {
        if(a==1)
        {
        cout<<"one ";
        break;
        }
        else if(a==2)
        {
        cout<<"two ";
        break;
        }
        else if(a==3)
        {
        cout<<"three ";
        break;
        }
        else if(a==4)
        {
        cout<<"four";
        break;
        }
        else if(a==5)
        {
        cout<<"five";
        break;
        }
        else if(a==6)
        {
        cout<<"six ";
        break;
        }
        else if(a==7)
        {
        cout<<"seven \n";
        break;
        }
        else if(a==8)
        {
        cout<<"eight \n";
        break;
        }
        else if(a==9)
        {
        cout<<"nine \n";
        break;
        }
        else 
        {
        cout<<"Greater than 9 \n";
        break;
        }
        
    }
    return 0;
}
